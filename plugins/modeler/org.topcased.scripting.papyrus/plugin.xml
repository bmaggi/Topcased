<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.topcased.scripting.getSelection">
      <getter
            class="org.topcased.scripting.papyrus.getter.GetSelectionFromGMF"
            name="FromGmfPart"
            priority="MEDIUM">
      </getter>
   </extension>
   <extension
         point="org.eclipse.papyrus.ui.perspectiveconfiguration">
      <!-- This contribution will not work untill Papyrus bug fix is published :
       https://bugs.eclipse.org/bugs/show_bug.cgi?id=309667
       For the moment, only one contribution is taken in acount. -->
      <configuration
            perspectiveID="org.eclipse.papyrus.core.perspective">
         <actionSet
               actionSetID="eclipsemonkeyAction.actionSet">
         </actionSet>
         <category
               categoryID="org.eclipse.core.commands.categories.autogenerated">
         </category>
         <category
               categoryID="eclipsemonkeyMenu">
         </category>
         <command
               commandID="AUTOGEN:::eclipsemonkeyAction.actionSet/org.eclipse.eclipsemonkey.action1">
         </command>
         <command
               commandID="AUTOGEN:::org.eclipse.eclipsemonkey.emFileContextMenu/org.eclipse.eclipsemonkey.action2">
         </command>
         <command
               commandID="AUTOGEN:::org.eclipse.eclipseMonkey.monkeyFolderContextMenu/org.eclipse.eclipsemonkey.action3">
         </command>
         <command
               commandID="AUTOGEN:::org.eclipse.eclipsemonkey.emFileContextMenu/org.eclipse.eclipsemonkey.action4">
         </command>
         <command
               commandID="AUTOGEN:::org.eclipse.eclipsemonkey.emFileContextMenu/org.eclipse.eclipsemonkey.action5">
         </command>
         <menu
               menuID="eclipsemonkeyMenu">
         </menu>
         <menu
               menuID="org.eclipse.eclipseMonkey.monkeyFolderContextMenu">
         </menu>
         <menu
               menuID="org.eclipse.eclipsemonkey.emFileContextMenu">
         </menu>
      </configuration>
   </extension>

</plugin>
